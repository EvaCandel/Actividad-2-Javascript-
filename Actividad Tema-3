//Ejercicio for

for(let i=0;i<=10;i++){
    console.log(i)
}
//Ejercicio while

let i = 0;
let max =10;
while (i<=10){
    console.log(i);
    i++;
}

//Ejercicio while, if y break

let factorial = 1;
let i = 1;
while (true) {
    factorial = factorial * i;
    if (i>10){
        break;
    }
    i++;
    console.log(factorial);
}
